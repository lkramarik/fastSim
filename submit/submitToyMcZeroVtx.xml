<?xml version="1.0" encoding="utf-8" ?>
<job nProcesses="40" simulateSubmission="false">

        <stdout URL="file:.&productionId;/jobs/log/$JOBID.log" />
        <stderr URL="file:&productionId;/jobs/err/$JOBID.err" />
                
        <Generator>
          <Location>./jobs/report/</Location>
          <ScriptLocation>./jobs/csh/</ScriptLocation>
          <ListLocation>./jobs/list/</ListLocation>
        </Generator>
                                                
	<ResourceUsage>
		<Memory>
			<MinMemory>4000</MinMemory>
		</Memory>
	</ResourceUsage>

<command>
starver SL16d

root4star -q -b -l toyMcDpm.C++\(1e6\)
mv Dpm.toyMc.root Dpm.$JOBID.toyMc.root
chgrp rhstar *.root
chmod g+rw *.root
</command>

<output fromScratch="D0.toy*.root" toURL="&codePath;/out/" />


<SandBox installer="ZIP">
  <Package name="LocalLibraries">
      <!--<File>file:./toyMcDpm.C</File>-->
      <!--<File>file:./input/</File>-->
      <File>file:&codePath;/toyMcEffZeroDecayLength.C</File>
      <File>file:&codePath;/Momentum_resolution_SL16d.root</File>
      <File>file:&codePath;/pp200_spectra.root</File>
      <File>file:&codePath;/Run14_D0_MyRaa_pT1.0.root</File>
      <File>file:&codePath;/HftRatio_AuAu2016_lumiprod.root</File>
      <File>file:&codePath;/Dca2D_AuAu2016_lumiprod.root</File>
      <File>file:&codePath;/Vz_Cent.root</File>
      <File>file:&codePath;/Eff_PionPlus_embedding.root</File>
      <File>file:&codePath;/Eff_PionMinus_embedding.root</File>
      <File>file:&codePath;/Eff_KaonPlus_embedding.root</File>
      <File>file:&codePath;/Eff_KaonMinus_embedding.root</File>
	</Package>
</SandBox>

</job>
